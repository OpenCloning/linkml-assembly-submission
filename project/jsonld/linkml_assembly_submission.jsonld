  "slots": [
    {
      "name": "id",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/id",
      "description": "The identifier for the object",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/id",
      "identifier": true,
      "owner": "Category",
      "domain_of": [
        "Category"
      ],
      "range": "string",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "title",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/title",
      "description": "A title for the representation of the object",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/title",
      "owner": "Assembly",
      "domain_of": [
        "Category",
        "Kit",
        "Assembly"
      ],
      "range": "string",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "description",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/description",
      "description": "A description of the object",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/description",
      "owner": "Assembly",
      "domain_of": [
        "Category",
        "Kit",
        "Sequence",
        "AddGenePlasmid",
        "Assembly"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "image",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/image",
      "description": "File name of an image for the object",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/image",
      "owner": "Category",
      "domain_of": [
        "Category"
      ],
      "range": "string",
      "pattern": "^[a-zA-Z0-9_\\-\\.]+\\.(png|jpg|jpeg|gif|svg)$",
      "@type": "SlotDefinition"
    },
    {
      "name": "pmid",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/pmid",
      "description": "The PubMed ID for the object",
      "examples": [
        {
          "value": "PMID:123456",
          "@type": "Example"
        }
      ],
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/pmid",
      "owner": "Kit",
      "domain_of": [
        "Kit"
      ],
      "range": "PMID_identifier",
      "pattern": "^PMID:\\d+$",
      "@type": "SlotDefinition"
    },
    {
      "name": "addgene_url",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/addgene_url",
      "description": "The Addgene URL for the kit",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/addgene_url",
      "owner": "Kit",
      "domain_of": [
        "Kit"
      ],
      "range": "https_identifier",
      "required": true,
      "pattern": "^https://www.addgene.org/.+$",
      "@type": "SlotDefinition"
    },
    {
      "name": "name",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/name",
      "description": "The name of a thing",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/name",
      "owner": "Oligo",
      "domain_of": [
        "Sequence",
        "Oligo"
      ],
      "range": "string",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "addgene_id",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/addgene_id",
      "description": "The Addgene ID for the plasmid",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/addgene_id",
      "identifier": true,
      "owner": "AddGenePlasmid",
      "domain_of": [
        "AddGenePlasmid"
      ],
      "range": "string",
      "required": true,
      "pattern": "^\\d+$",
      "@type": "SlotDefinition"
    },
    {
      "name": "category",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/category",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/category",
      "owner": "Sequence",
      "domain_of": [
        "Sequence"
      ],
      "range": "Category",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "resistance",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/resistance",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/resistance",
      "owner": "AddGenePlasmid",
      "domain_of": [
        "AddGenePlasmid"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "well",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/well",
      "description": "The well where a plasmid is located in a plate",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/well",
      "owner": "AddGenePlasmid",
      "domain_of": [
        "AddGenePlasmid"
      ],
      "range": "string",
      "@type": "SlotDefinition"
    },
    {
      "name": "full_name",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/full_name",
      "description": "The full name of the submitter, will be ignored if the name can be taken from ORCID",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/full_name",
      "owner": "Submitter",
      "domain_of": [
        "Submitter"
      ],
      "range": "string",
      "required": true,
      "@type": "SlotDefinition"
    },
    {
      "name": "orcid",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/orcid",
      "description": "The ORCID of the submitter",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/orcid",
      "owner": "Submitter",
      "domain_of": [
        "Submitter"
      ],
      "range": "orcid_identifier",
      "pattern": "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[\\dX]$",
      "@type": "SlotDefinition"
    },
    {
      "name": "github_username",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/github_username",
      "description": "The GitHub username of the submitter",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/github_username",
      "owner": "Submitter",
      "domain_of": [
        "Submitter"
      ],
      "range": "string",
      "pattern": "^[a-zA-Z0-9-]+$",
      "@type": "SlotDefinition"
    },
    {
      "name": "type",
      "definition_uri": "https://w3id.org/genestorian/linkml-assembly-submission/type",
      "description": "The type of sequence",
      "from_schema": "https://w3id.org/genestorian/linkml-assembly-submission",
      "slot_uri": "https://w3id.org/genestorian/linkml-assembly-submission/type",
      "designates_type": true,
      "owner": "Sequence",
      "domain_of": [
