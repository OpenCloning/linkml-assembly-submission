{
    "$defs": {
        "Assembly": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "description": {
                    "description": "A description of the object",
                    "type": "string"
                },
                "fragment_order": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "template_file": {
                    "pattern": "^.*.json$",
                    "type": "string"
                },
                "title": {
                    "description": "A title for the representation of the object",
                    "type": "string"
                }
            },
            "required": [
                "title"
            ],
            "title": "Assembly",
            "type": "object"
        },
        "Category": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "description": {
                    "description": "A description of the object",
                    "type": "string"
                },
                "id": {
                    "description": "The identifier for the object",
                    "type": "string"
                },
                "image": {
                    "description": "File name of an image for the object",
                    "pattern": "^[a-zA-Z0-9_\\-\\.]+\\.(png|jpg|jpeg|gif|svg)$",
                    "type": "string"
                },
                "title": {
                    "description": "A title for the representation of the object",
                    "type": "string"
                }
            },
            "required": [
                "id",
                "title"
            ],
            "title": "Category",
            "type": "object"
        },
        "Kit": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "addgene_url": {
                    "description": "The Addgene URL for the kit",
                    "pattern": "^https://www.addgene.org/.+$",
                    "type": "string"
                },
                "description": {
                    "description": "A description of the object",
                    "type": "string"
                },
                "pmid": {
                    "description": "The PubMed ID for the object",
                    "pattern": "^PMID:\\d+$",
                    "type": "string"
                },
                "title": {
                    "description": "A title for the representation of the object",
                    "type": "string"
                }
            },
            "required": [
                "addgene_url",
                "title",
                "description"
            ],
            "title": "Kit",
            "type": "object"
        },
        "Sequence": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "addgene_id": {
                    "description": "The Addgene ID for the plasmid",
                    "pattern": "^\\d+$",
                    "type": "string"
                },
                "category": {
                    "type": "string"
                },
                "description": {
                    "description": "A description of the object",
                    "type": "string"
                },
                "plasmid_name": {
                    "type": "string"
                },
                "resistance": {
                    "type": "string"
                },
                "well": {
                    "description": "The well where a plasmid is located in a plate",
                    "type": "string"
                }
            },
            "required": [
                "addgene_id",
                "category"
            ],
            "title": "Sequence",
            "type": "object"
        },
        "Submission": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "assemblies": {
                    "items": {
                        "$ref": "#/$defs/Assembly"
                    },
                    "type": "array"
                },
                "categories": {
                    "items": {
                        "$ref": "#/$defs/Category"
                    },
                    "type": "array"
                },
                "kit": {
                    "$ref": "#/$defs/Kit"
                },
                "sequences": {
                    "items": {
                        "$ref": "#/$defs/Sequence"
                    },
                    "type": "array"
                },
                "submitters": {
                    "items": {
                        "$ref": "#/$defs/Submitter"
                    },
                    "type": "array"
                }
            },
            "required": [
                "submitters",
                "kit",
                "sequences",
                "categories",
                "assemblies"
            ],
            "title": "Submission",
            "type": "object"
        },
        "Submitter": {
            "additionalProperties": false,
            "description": "",
            "properties": {
                "full_name": {
                    "description": "The full name of the submitter, will be ignored if the name can be taken from ORCID",
                    "type": "string"
                },
                "github_username": {
                    "description": "The GitHub username of the submitter",
                    "pattern": "^[a-zA-Z0-9-]+$",
                    "type": "string"
                },
                "orcid": {
                    "description": "The ORCID of the submitter",
                    "pattern": "^\\d{4}-\\d{4}-\\d{4}-\\d{3}[\\dX]$",
                    "type": "string"
                }
            },
            "required": [
                "full_name"
            ],
            "title": "Submitter",
            "type": "object"
        }
    },
    "$id": "https://w3id.org/genestorian/linkml-assembly-submission",
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "additionalProperties": true,
    "description": "",
    "metamodel_version": "1.7.0",
    "properties": {
        "assemblies": {
            "items": {
                "$ref": "#/$defs/Assembly"
            },
            "type": "array"
        },
        "categories": {
            "items": {
                "$ref": "#/$defs/Category"
            },
            "type": "array"
        },
        "kit": {
            "$ref": "#/$defs/Kit"
        },
        "sequences": {
            "items": {
                "$ref": "#/$defs/Sequence"
            },
            "type": "array"
        },
        "submitters": {
            "items": {
                "$ref": "#/$defs/Submitter"
            },
            "type": "array"
        }
    },
    "required": [
        "submitters",
        "kit",
        "sequences",
        "categories",
        "assemblies"
    ],
    "title": "linkml-assembly-submission",
    "type": "object",
    "version": null
}